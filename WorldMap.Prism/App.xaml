<Application x:Class="WorldMap.Prism.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WorldMap.Prism">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WorldMap.WpfTheme;Component/Themes/Color.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WorldMap.WpfTheme;Component/Themes/Icon.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WorldMap.WpfTheme;Component/Themes/Font.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WorldMap.WpfTheme;Component/Themes/Button.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
